	<body>
		<div id="wrap">
			<div id="header">
				
				<table >
					<tr>
						<!--<td width="10px"></td>-->
						<td align="left" width="35px"><div style="margin-left: 10px;margin-top: 5px"><img src="../../image/statusbar/back.png" height="32px" width="32px" onclick="backTo()"/></div></td>
						<td align="center"><div id="productName" style="margin-top:-5px;margin-left:10px;line-height: 30px;font-size:22px;">我的录入</div></td>
						<td align="right" width="35px"><img src="../../image/scancode/home_page.png" height="32px" width="32px" onclick="homepage()"/></td>
					</tr>
				</table>
			</div>
			<div id = "main"></div>
		</div>
	</body>
	<script type="text/javascript" src="../../script/api.js"></script>
	<script type="text/javascript" src="../../script/bts_common.js"></script>
	<script type="text/javascript">
		function backTo() {
			api.closeWin({
			});
		}

		function backToHome() {
			api.closeToWin({
				name : 'root'
			});
		}

		
		apiready = function() {
		
		var header = $api.byId('header');
		var headerPos = $api.offset(header);
			var params = api.pageParam;
			api.openFrame({
				name : 'myenter_header',
				url : './myenter_header.html',
				rect : {
					x : 0,
					y : headerPos.h,
					w : 'auto',
					h : 'auto'
				},
				bounces : false,
				hScrollBarEnabled : false
			});
			//			getProductDetail('6949621101458');
			var params = api.pageParam;
//			getProductDetail(params.productcode);
		};
		
		function homepage() {

			api.closeToWin({
	            name: 'root'
            });
		}

		
